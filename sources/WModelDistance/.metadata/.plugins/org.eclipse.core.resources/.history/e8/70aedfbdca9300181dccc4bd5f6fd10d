package PacmanGame.search;

public final class PacmanGameDistanceUtilFactory {

	private static PacmanDistanceUtil INSTANCE;
	
	public synchronized static PacmanDistanceUtil getInstance() {
		if(INSTANCE == null){
	        synchronized (PacmanDistanceUtil.class) {
	            if(INSTANCE == null){
	            	INSTANCE = new PacmanDistanceUtil();
	            }
	        }
	    }
	    return INSTANCE;
	}
}
